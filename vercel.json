{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
          },
          {
            "src": "/(.*)",
            "dest": "/"
          }
          
    ]
}


